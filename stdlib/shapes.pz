func triangle(x, y)
	x := 1
	y := x*0.866
	shape(:triangle, x, y, 1)

func cube(x, y, z)
	x := 1
	y := x
	z := y
	shape(:cube, x, y, z)

func sphere(x, y, z)
	x := 1
	y := x
	z := y
	shape(:sphere, x, y, z)

func cylinder(x, y, z)
	x := 1
	y := x
	z := y
	shape(:cylinder, x, y, z)

func rectangle(x, y)
	x := 1
	y := x
	shape(:rectangle, x, y, 1)

func line(x)
	x := 1
	shape(:line, x, 1, 1)

func geo(name, x, y, z)
	x := 1
	y := x
	z := y
	shape(name, x, y, z)
